{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "outDir": "dist",
    "typeRoots": ["./node_modules/@types", "./internal/utils"],
    "types": ["node", "express", "jest"],
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["cmd", "cmd/server/public"]
}